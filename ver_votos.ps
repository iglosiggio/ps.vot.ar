/fontsize 8 def
/Helvetica findfont fontsize scalefont setfont

/xpos 20 def
/ypos 40 def

/newline {
  /ypos ypos fontsize add def
  ypos 790 gt {
    % si me fui de la pagina cambio de columna
    /xpos xpos 300 add def
    /ypos 40 def
  } if
  xpos ypos moveto
} def

userdict /votos known not {
  /votos [
    [ (PRUEBA) (19/09/2017 10:03:22) ]
    [ (PRUEBA) (19/09/2017 10:05:37) ]
    [ (PRUEBA) (19/09/2017 10:05:50) ]
    [ (PRUEBA) (19/09/2017 10:06:12) ]
    [ (PRUEBA) (19/09/2017 10:11:42) ]
    [ (PRUEBA) (19/09/2017 15:09:33) ]
    [ (PRUEBA) (19/09/2017 15:15:22) ]
    [ (BLANCO) (19/09/2017 15:17:07) ]
  ] def
} if % si no hay votos muestro la pagina de prueba

xpos ypos moveto

votos {
  dup type /arraytype eq
  {
    dup 0 get show
    xpos 100 add  ypos moveto
    dup 1 get show
    newline
  } if % si el voto es valido lo imprimo (sino probablemente no se asigno)
} forall

showpage
